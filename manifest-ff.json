{
    "name": "Esportal Browser Boost",
    "version": "0.0.0.11",
    "manifest_version": 2,
    "description": "Remove the twitch section from esportal.com",
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://esportal.com/*"],
            "js": ["src/content-script.js"]
        }
    ],
    "browser_specific_settings": {
        "gecko": {
          "id": "esportal-browser-boost"
        }
    }
}